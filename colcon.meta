{
    "names": {
        "rcutils": {
            "cmake-args": [
                "-DENABLE_TESTING=OFF",
                "-DRCUTILS_NO_FILESYSTEM=ON",
                "-DRCUTILS_NO_THREAD_SUPPORT=ON",
                "-DRCUTILS_NO_64_ATOMIC=ON",
                "-DRCUTILS_AVOID_DYNAMIC_ALLOCATION=ON",
                "-DRCUTILS_CUSTOM_TIME_IMPL=ON"
            ]
        },
        "microxrcedds_client": {
            "cmake-args": [
                "-DUCLIENT_PIC=OFF",
                "-DUCLIENT_PROFILE_UDP=OFF",
                "-DUCLIENT_PROFILE_TCP=OFF",
                "-DUCLIENT_PROFILE_SERIAL=ON",
                "-DUCLIENT_PROFILE_STREAM_FRAMING=ON",
                "-DUCLIENT_PROFILE_CUSTOM_TRANSPORT=ON",
                "-DUCLIENT_PLATFORM_NAME=riscv",
                "-DUCLIENT_EXTERNAL_TCP=OFF",
                "-DUCLIENT_EXTERNAL_UDP=OFF"
            ]
        },
        "rmw_microxrcedds": {
            "cmake-args": [
                "-DRMW_UXRCE_MAX_NODES=1",
                "-DRMW_UXRCE_MAX_PUBLISHERS=5",
                "-DRMW_UXRCE_MAX_SUBSCRIPTIONS=5",
                "-DRMW_UXRCE_MAX_SERVICES=1",
                "-DRMW_UXRCE_MAX_CLIENTS=1",
                "-DRMW_UXRCE_MAX_HISTORY=4",
                "-DRMW_UXRCE_TRANSPORT=custom"
            ]
        },
        "tracetools": {
            "cmake-args": [
                "-DTRACETOOLS_DISABLED=ON",
                "-DTRACETOOLS_STATUS_CHECKING_TOOL=OFF"
            ]
        },
        "rosidl_typesupport": {
            "cmake-args": [
                "-DROSIDL_TYPESUPPORT_SINGLE_TYPESUPPORT=ON"
            ]
        },
        "rcl": {
            "cmake-args": [
                "-DBUILD_TESTING=OFF",
                "-DRCL_COMMAND_LINE_ENABLED=OFF",
                "-DRCL_LOGGING_ENABLED=OFF"
            ]
        },
        "rmw": {
            "cmake-args": [
                "-DRMW_IMPLEMENTATION=rmw_microxrcedds"
            ]
        },
        "rosidl_runtime_c": {
            "cmake-args": [
                "-DROSIDL_RUNTIME_C_BUILDING_DLL=OFF"
            ]
        },
        "rosidl_generator_c": {
            "cmake-args": [
                "-DROSIDL_GENERATOR_C_BUILDING_DLL=OFF"
            ]
        },
        "rosidl_typesupport_microxrcedds_c": {
            "cmake-args": [
                "-DROSIDL_TYPESUPPORT_MICROXRCEDDS_C_BUILDING_DLL=OFF"
            ]
        },
        "microcdr": {
            "cmake-args": [
                "-DUCDR_ISOLATED_BUILD=ON"
            ]
        }
    }
}
